r="0,0,0"/><top-border width="1" style="solid" color="0,0,0"/><bottom-border width="1" style="solid" color="0,0,0"/></cell-style><dataset-value dataset-name="contract_sql" aggregate="group" property="other" order="none" mapping-type="simple"></dataset-value></cell><row row-number="1" height="18" band="title"/><row row-number="2" height="18"/><column col-number="1" width="121"/><column col-number="2" width="80"/><column col-number="3" width="80"/><column col-number="4" width="80"/><column col-number="5" width="74"/><column col-number="6" width="116"/><column col-number="7" width="112"/><column col-number="8" width="91"/><column col-number="9" width="87"/><column col-number="10" width="74"/><column col-number="11" width="74"/><datasource name="内置数据源" type="buildin"><dataset name="contract_sql" type="sql"><sql><![CDATA[${
" SELECT " +
" a.contract_id, " +
" a.contract_code, " +
" a.contract_amount, " +
" a.gross_margin, " +
" a.gross_profit_margin, " +
" a.terms_of_payment, " +
" a.terms_of_warranty, " +
" a.contract_start_time, " +
" a.contract_end_time, " +
" a.training_or_outsourcing, " +
" a.other, " +
" apply.project_name, " +
" apply.project_code, " +
" apply.category, " +
" apply.estimated_contract_amount, " +
" apply.estimated_gross_profit_margin, " +
" contract.project_id, " +
" u1.NAME AS projectManagerName, " +
" u4.NAME AS salerName, " +
" u4.login_name AS salerLoginName, " +
" s5.NAME AS salerOfficeName, " +
" a.id, " +
" a.create_by, " +
" a.create_date, " +
" a.update_by, " +
" a.update_date, " +
" a.remarks, " +
" contract.proc_ins_id, " +
" contract.proc_status  " +
" FROM " +
" project_contract_item a " +
" LEFT JOIN project_contract contract ON contract.id = a.contract_id " +
" LEFT JOIN project_apply_external apply ON apply.id = contract.project_id " +
" LEFT JOIN sys_user u1 ON u1.id = contract.project_manager_id " +
" LEFT JOIN sys_user u4 ON u4.id = apply.saler_id " +
" LEFT JOIN sys_office s5 ON s5.id = u4.office_id  " +
" WHERE " +
" a.del_flag = \'0\'  " +

(emptyparam("pc")==false ? "and apply.project_code like concat(\'%\', :pc, \'%\')" : "") +
(emptyparam("pn")==false ? "and apply.project_name like concat(\'%\', :pn, \'%\')" : "") +
(emptyparam("cc")==false ? "and contract.contract_code like concat(\'%\', :cc, \'%\')" : "") +
(emptyparam("qb")==false && emptyparam("qe")==false  ? "and a.update_date BETWEEN :qb AND :qe" : "") +
(emptyparam("qb")==true && emptyparam("qe")==false  ? "and a.update_date <= :qe" : "") +
(emptyparam("qb")==false && emptyparam("qe")==true  ? "and a.update_date >= :qb" : "") +
" ORDER BY " +
" a.update_date"
}]]></sql><field name="contract_id"/><field name="contract_code"/><field name="contract_amount"/><field name="gross_margin"/><field name="gross_profit_margin"/><field name="terms_of_payment"/><field name="terms_of_warranty"/><field name="contract_start_time"/><field name="contract_end_time"/><field name="training_or_outsourcing"/><field name="other"/><field name="project_name"/><field name="project_code"/><field name="category"/><field name="estimated_contract_amount"/><field name="estimated_gross_profit_margin"/><field name="project_id"/><field name="projectManagerName"/><field name="salerName"/><field name="salerLoginName"/><field name="salerOfficeName"/><field name="id"/><field name="create_by"/><field name="create_date"/><field name="update_by"/><field name="update_date"/><field name="remarks"/><field name="proc_ins_id"/><field name="proc_status"/><parameter name="pc" type="String" default-value=""/><parameter name="pn" type="String" default-value=""/><parameter name="cc" type="String" default-value=""/><parameter name="qb" type="Date" default-value=""/><parameter name="qe" type="Date" default-value=""/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
