  <process id="ProjectApplyExternal" name="项目立项审批流程" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="end" class="com.thinkgem.jeesite.modules.project.utils.AuditEndListener"></activiti:executionListener>
      <activiti:executionListener event="start" class="com.thinkgem.jeesite.modules.project.utils.AuditStartListener"></activiti:executionListener>
    </extensionElements>
    <startEvent id="startevent1" name="Start" activiti:initiator="apply" activiti:formKey="/apply/external/projectApplyExternal/form"></startEvent>
    <userTask id="usertask_business_leader" name="申请部门领导审批" activiti:assignee="${assigneeService.findLeader(apply)}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask_boss" name="总经理审批" activiti:candidateGroups="usertask_boss" activiti:skipExpression="${execution.getVariable('skip_boss') == 1}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask_owner" name="申请人立项申请" activiti:assignee="${apply}"></userTask>
    <sequenceFlow id="flow57" sourceRef="startevent1" targetRef="usertask_owner"></sequenceFlow>
    <sequenceFlow id="flow65" sourceRef="usertask_boss" targetRef="endevent1"></sequenceFlow>
    <userTask id="usertask_tech_leader" name="技术开发中心审批" activiti:candidateGroups="usertask_tech_leader" activiti:skipExpression="${execution.getVariable('skip_dev') == 1}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask_market_leader" name="市场营销中心审批" activiti:candidateGroups="usertask_market_leader">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask_finance_leader" name="财务部门审批" activiti:candidateGroups="usertask_finance_leader">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask_market_boss" name="营销中心分管审批" activiti:assignee="${assigneeService.findMarketBoss(apply)}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow66" sourceRef="usertask_owner" targetRef="usertask_business_leader"></sequenceFlow>
    <sequenceFlow id="flow67" sourceRef="usertask_business_leader" targetRef="usertask_finance_leader"></sequenceFlow>
    <sequenceFlow id="flow68" sourceRef="usertask_finance_leader" targetRef="usertask_tech_leader"></sequenceFlow>
    <sequenceFlow id="flow69" sourceRef="usertask_tech_leader" targetRef="usertask_market_leader"></sequenceFlow>
    <sequenceFlow id="flow70" sourceRef="usertask_market_leader" targetRef="usertask_market_boss"></sequenceFlow>
    <sequenceFlow id="flow71" sourceRef="usertask_market_boss" targetRef="usertask_boss"></sequenceFlow>
  </process>
