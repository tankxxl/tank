    </extensionElements>
    <startEvent id="startevent1" name="Start" activiti:initiator="apply" activiti:formKey="/project/finish/projectFinishApproval/form"></startEvent>
    <userTask id="usertask_business_leader" name="申请部门领导审批" activiti:assignee="${assigneeService.findLeader(apply)}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.thinkgem.jeesite.modules.apply.utils.MailTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask_business_boss" name="分管领导审批" activiti:assignee="${assigneeService.findBusiBoss(apply)}">
